{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "declaration": true,
    "outDir": "dist",
    
  },
  "include": ["lib/**/*.ts", "contracts-abis-exports/**/*.ts"],
  "exclude": ["node_modules", "dist"]
}
